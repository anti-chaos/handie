{
  "name": "handie",
  "version": "1.0.0",
  "description": "为前端开发提供统一的布局、组件和工具方法",
  "private": true,
  "main": "index.js",
  "directories": {
    "example": "site",
    "test": "test"
  },
  "scripts": {
    "ascii": "node ./build/scripts/ascii.js",
    "modulize": "modulizer --in .handie-tmp/components/templates --out .handie-tmp/components/javascripts --npm-name handie-components --npm-version 0.0.0 --force",
    "cc": "gulp convert-components",
    "serve": "polymer serve",
    "prestart": "npm run compile",
    "start": "polymer serve",
    "precopy:jquery": "rm -rf test/jquery/assets",
    "copy:jquery": "cp -R dist/jquery test/jquery/assets",
    "pretest": "npm run copy:jquery",
    "test": "http-server ./test --cors='*'",
    "major:core": "cd src/core && npm version major && cd ../..",
    "minor:core": "cd src/core && npm version minor && cd ../..",
    "patch:core": "cd src/core && npm version patch && cd ../..",
    "babel:core": "babel src/core/utils -d dist/core/utils && babel src/core/adaptors -d dist/core/adaptors",
    "precompile:core": "rm -rf dist/core",
    "compile:core": "gulp compile-core",
    "pack:core": "cp src/core/*.json src/core/*.md dist/core",
    "prerelease:core": "npm run compile:core && npm run pack:core",
    "release:core": "cd dist/core && npm publish",
    "up2qiniu": "node ./build/scripts/upload.js qiniu",
    "up2wantu": "node ./build/scripts/upload.js wantu"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:ourai/handie.git"
  },
  "keywords": [
    "ui",
    "dashboard",
    "admin",
    "website"
  ],
  "author": "Ourai Lin <ourairyu@gmail.com> (http://ourai.ws/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ourai/handie/issues"
  },
  "homepage": "https://ourai.github.io/handie/",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-banner": "^0.1.3",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "gulp-shopify-sass": "^0.3.3",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-strip-comments": "^2.4.5",
    "gulp-strip-css-comments": "^1.2.0",
    "gulp-template": "^5.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp-umd": "^0.2.1",
    "gulp-wrap": "^0.13.0",
    "http-server": "^0.11.1",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-cleanup": "^3.0.0-beta.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  }
}
